
<div id="Profile"> 

<!-- if not admin show profile page, if admin show new dashboard and wrote this here -->
   <div class= "panel">
<div class="Profile-header">    
  <h2>Profile Page </h2>
</div>

  <%= image_tag @user.avatar.url, :class => 'img-thumbnail' %>
      <div class="Profile-line">  
        <strong>First name:</strong>
        <%= @user.first_name %>
    </div>

    <div class="Profile-line">  
        <strong>Last name:</strong>
        <%= @user.last_name %>
    </div>

    <div class="Profile-line">  
        <strong>Email:</strong>
        <%= @user.email %>
    </div>

    <div class="Profile-line">  
        <strong>Occupation:</strong>
        <%= @user.occupation %>
    </div>

    <div class="Profile-line">  
        <strong>Location:</strong>
        <%= @user.location %>
    </div>

    <div class="Profile-line">  
        <strong>About:</strong>
        <%= @user.about %>
    </div>
    <div class="Profile-line">  
        <strong>Skills:</strong>
          <%= @user.skills.join(', ') %>
    </div>



</div>
  </div>
  </div>




